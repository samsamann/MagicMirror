{
  "name": "magic-mirror-frontend",
  "version": "0.1.0",
  "description": "Frontend of the MagicMirror",
  "repository": {
    "type": "git",
    "url": "https://github.com/samsamann/MagicMirror"
  },
  "contributors": [
    "Samuel Ackermann",
    "Benjamin Ackermann"
  ],
  "license": "MIT",
  "private": true,
  "main": "dist/app.js",
  "scripts": {
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "start": "tsc -p src/tsconfig.electron.json && npm run build:dev",
    "build:dev": "ng build && npm-run-all -p ng:build:dev electron:dev",
    "build:prod": "tsc -p src/tsconfig.electron.json && npm run ng:build:prod",
    "electron:dev": "electron . --dev",
    "ng:build:dev": "ng build --watch",
    "ng:build:prod": "ng build -c production --prod"
  },
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "@ngrx/effects": "^7.2.0",
    "@ngrx/store": "^7.2.0",
    "core-js": "^2.5.4",
    "electron": "^2.0.18",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/cli": "~7.3.1",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "electron-reload": "^1.4.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "npm-run-all": "^4.1.5",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2"
  }
}
